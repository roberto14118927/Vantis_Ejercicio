# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'Vantis' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
  pod 'Alamofire'
  pod 'SwiftyJSON'
  pod 'AlamofireImage'
  
  # Pods for Vantis

  target 'VantisTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'VantisUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
